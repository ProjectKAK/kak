<div class="jumbotron" id="static">
  <div class="container">
    <div class="form">

      <%= bootstrap_form_for @user do |f| %>
          <div>
          <%= f.text_field :email, placeholder: "Email" %>
          </div>
          <div>
            <%= f.password_field :password, placeholder: "Password" %>
          </div>

          <div>
            <%= f.password_field :password, placeholder: "Confirm Password" %>
          </div>

          <div>
            <%= f.submit "Signup"%>
          </div>
      <% end %>
    
    </div>
  </div>
</div>
      